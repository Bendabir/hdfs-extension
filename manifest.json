{
    "manifest_version": 2,
    "name": "HDFS UI Upper Directory",
    "description": "Refreshes the Airlfow DAG run view automagically.",
    "version": "0.1.0",
    "icons": {
        "48": "resources/icons/up_icon.png"
    },
    "permissions": [
        "*://*/*"
    ],
    "content_scripts": [{
        "matches": ["*://*/*"],
        "run_at": "document_end",
        "js": [
            "upper_directory.js"
        ]
    }]
}
